  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Shaprk\\Sms\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shaprk\\Sms\\SmsServiceProvider"
      ]
